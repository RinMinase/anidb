{
  "name": "rin-anidb",
  "description": "Rin Minase's Anime Database using AngularJS in ES6 compiled for Android with Cordova and deployed to Firebase using Travis CI",
  "homepage": "https://rin-anidb.firebaseapp.com/",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Rin Minase",
  "repository": {
    "type": "git",
    "url": "https://github.com/RinMinase/anidb"
  },
  "main": "index.js",
  "scripts": {
    "start": "gulp serve",
    "build": "gulp serve:dist",
    "bundle": "gulp bundle && cordova build android && adb install -r build/com.minase.anidb_0.1.0.apk",
    "deploy": "gulp build && firebase deploy",
    "lint": "eslint --ext .js src\\app",
    "test": "nightwatch --config src/assets/testing/nightwatch.conf.js",
    "setup": "node src/assets/testing/nightwatch.conf.js && cordova add platform android && yarn",
    "setup-android": "cordova add platform android && yarn",
    "setup-testing": "node src/assets/testing/nightwatch.conf.js"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.21",
    "angular": "1.7.6",
    "angular-animate": "^1.7.6",
    "angular-chart.js": "^1.1.1",
    "angular-hammer": "^2.2.0",
    "angular-sweetalert": "^1.1.2",
    "animatewithsass": "^3.2.1",
    "awesome-bootstrap-checkbox": "^1.0.1",
    "bluebird": "^3.5.3",
    "bootstrap": "^4.2.1",
    "firebase": "^5.8.1",
    "font-awesome": "^4.7.0",
    "fuse.js": "^3.3.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "oclazyload": "^1.1.0",
    "roboto-fontface": "^0.10.0",
    "ui-bootstrap4": "^3.0.6",
    "velocity-animate": "^1.5.2",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-loader": "8.0.5",
    "browser-sync": "^2.26.3",
    "browser-sync-spa": "^1.0.3",
    "chromedriver": "^2.45.0",
    "cordova-android": "^7.1.4",
    "cordova-plugin-whitelist": "^1.3.3",
    "del": "^3.0.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.12.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-angular": "^4.0.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.0",
    "gulp-angular-templatecache": "^2.2.6",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-filter": "^5.1.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-inject": "^5.0.2",
    "gulp-inject-string": "^1.1.2",
    "gulp-rev": "^9.0.0",
    "gulp-rev-replace": "^0.4.2",
    "gulp-sass": "^4.0.2",
    "gulp-sass-lint": "^1.4.0",
    "gulp-useref": "^3.1.6",
    "html-loader": "^0.5.5",
    "ng-annotate-loader": "^0.6.1",
    "ng-annotate-patched": "^1.8.1",
    "ngtemplate-loader": "^2.0.1",
    "nightwatch": "^1.0.18",
    "undertaker-forward-reference": "^1.0.2",
    "vinyl-paths": "^2.1.0",
    "webpack": "^4.29.0",
    "webpack-stream": "^5.2.1"
  },
  "resolutions": {
    "**/@firebase/app-types": "0.3.2",
    "**/angular": "1.7.6",
    "**/gulp": "4.0.0",
    "**/jquery": "3.3.1",
    "**/popper.js": "1.14.6"
  },
  "engines": {
    "node": "^6.14.0 || ^8.10.0 || >=9.10.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android"
    ]
  },
  "eslintIgnore": [
    "src/assets/firebase/*"
  ]
}
