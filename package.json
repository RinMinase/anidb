{
  "name": "rin-anidb",
  "description": "Rin Minase's Anime Database using AngularJS in ES6 compiled for Android with Cordova and deployed to Firebase using Travis CI",
  "homepage": "https://rin-anidb.firebaseapp.com/",
  "version": "0.2.0",
  "license": "MIT",
  "author": "Rin Minase",
  "private": true,
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/RinMinase/anidb"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod && sw-precache --root=dist --config=src/assets/config/sw-precache-config.js",
    "bundle": "rm -rf www && mkdir www && npm run build && rm -rf www && mv dist www && cordova build android && rm -rf www",
    "test": "ng test",
    "lint": "ng lint",
    "setup": "cordova platform add android && yarn"
  },
  "dependencies": {
    "@angular/animations": "^7.2.3",
    "@angular/common": "^7.2.0",
    "@angular/compiler": "^7.2.3",
    "@angular/core": "^7.2.3",
    "@angular/forms": "^7.2.3",
    "@angular/platform-browser": "^7.2.3",
    "@angular/platform-browser-dynamic": "^7.2.3",
    "@angular/router": "^7.2.3",
    "@ng-bootstrap/ng-bootstrap": "^4.0.2",
    "animatewithsass": "^3.2.1",
    "bluebird": "^3.5.3",
    "bootstrap": "^4.2.1",
    "cordova-android": "^7.0.0",
    "firebase": "^5.8.2",
    "font-awesome": "^4.7.0",
    "fuse.js": "^3.3.0",
    "moment": "^2.24.0",
    "roboto-fontface": "^0.10.0",
    "rxjs": "^6.3.3",
    "sweetalert2": "^8.0.5",
    "xlsx": "^0.14.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.0",
    "@angular/cli": "^7.3.0",
    "@angular/compiler-cli": "^7.2.3",
    "@types/bluebird": "^3.5.25",
    "@types/jasmine": "^3.3.8",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^10.12.18",
    "codelyzer": "^4.5.0",
    "cordova-android": "^7.0.0",
    "cordova-plugin-whitelist": "^1.3.3",
    "jasmine-core": "^3.3.0",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "sw-precache": "^5.2.1",
    "tslint": "^5.12.1",
    "typescript": ">=3.1.1 <3.3"
  },
  "resolutions": {
		"**/@firebase/app-types": "0.3.3",
		"**/@firebase/auth-types": "0.5.1",
		"**/@firebase/firestore-types": "1.0.1",
		"**/@firebase/messaging-types": "0.2.4",
		"**/@firebase/storage-types": "0.2.4",
		"**/@firebase/util": "0.2.6",
    "**/jquery": "3.3.1",
    "**/popper.js": "1.14.7"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android"
    ]
  }
}
