{
  "name": "rin-anidb",
  "description": "Rin Minase's AniDB using Preact with Vite and deployed to Netlify using Circle CI",
  "homepage": "https://anidb.netlify.com/",
  "version": "0.3.0",
  "license": "MIT",
  "author": "Rin Minase",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/RinMinase/anidb"
  },
  "scripts": {
    "dev": "npm start",
    "start": "vite --port 3000 --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "cz": "cz"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@hookform/resolvers": "^2.9.7",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.2",
    "axios": "^0.27.2",
    "preact": "^10.9.0",
    "preact-async-route": "^2.2.1",
    "preact-router": "^4.1.0",
    "react-hook-form": "^7.34.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.3.0",
    "commitizen": "^4.2.5",
    "cz-emoji-conventional": "^1.0.1",
    "devmoji": "^2.3.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.4",
    "vite-plugin-pwa": "^0.12.3",
    "vite-tsconfig-paths": "^3.5.0",
    "yorkie": "^2.0.0"
  },
  "engines": {
    "node": "^16.0.0 || ^17.0.0",
    "yarn": "^1.19.1"
  },
  "gitHooks": {
    "prepare-commit-msg": "devmoji -e"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji-conventional"
    }
  },
  "prettier": {
    "printWidth": 80,
    "bracketSpacing": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "endOfLine": "lf"
  }
}
