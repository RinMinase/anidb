{
  "name": "rin-anidb",
  "description": "Rin Minase's Anime Database using AngularJS in ES6 compiled for Android with Cordova and deployed to Firebase using Travis CI",
  "homepage": "https://rin-anidb.firebaseapp.com/",
  "version": "0.2.0",
  "license": "MIT",
  "author": "Rin Minase",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/RinMinase/anidb"
  },
  "scripts": {
    "start": "gulp serve",
    "build": "gulp serve:dist",
    "bundle": "gulp bundle && cordova build android && adb install -r build/com.minase.anidb_0.1.0.apk",
    "deploy": "gulp build && firebase deploy",
    "lint": "eslint --ext .js src\\app",
    "test": "nightwatch --config src/assets/testing/nightwatch.conf.js",
    "setup": "node src/assets/testing/nightwatch.conf.js && cordova platform add android && yarn",
    "setup-android": "cordova platform add android && yarn",
    "setup-testing": "node src/assets/testing/nightwatch.conf.js"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.22",
    "angular": "1.7.7",
    "angular-animate": "^1.7.7",
    "angular-chart.js": "^1.1.1",
    "angular-hammer": "^2.2.0",
    "angular-sweetalert": "^1.1.2",
    "animatewithsass": "^3.2.1",
    "awesome-bootstrap-checkbox": "^1.0.1",
    "bluebird": "^3.5.3",
    "bootstrap": "^4.2.1",
    "firebase": "^5.8.2",
    "font-awesome": "^4.7.0",
    "fuse.js": "^3.3.0",
    "lodash": "^4.17.11",
    "moment-mini": "^2.22.1",
    "oclazyload": "^1.1.0",
    "popper.js": "1.14.7",
    "roboto-fontface": "^0.10.0",
    "rxjs": "^6.3.3",
    "sweetalert2": "^8.2.4",
    "xlsx": "^0.14.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.3",
    "@angular/cli": "^7.3.3",
    "@angular/compiler-cli": "^7.2.6",
    "@angular/platform-browser-dynamic": "^7.2.6",
    "@types/jasmine": "^3.3.9",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^11.9.5",
    "codelyzer": "^4.5.0",
    "cordova-android": "^8.0.0",
    "cordova-plugin-whitelist": "^1.3.3",
    "del": "^3.0.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.13.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-angular": "^4.0.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.0",
    "gulp-angular-templatecache": "^2.2.6",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-filter": "^5.1.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-inject": "^5.0.2",
    "gulp-inject-string": "^1.1.2",
    "gulp-rev": "^9.0.0",
    "gulp-rev-replace": "^0.4.2",
    "gulp-sass": "^4.0.2",
    "gulp-sass-lint": "^1.4.0",
    "gulp-useref": "^3.1.6",
    "html-loader": "^0.5.5",
    "ng-annotate-loader": "^0.6.1",
    "ng-annotate-patched": "^1.9.0",
    "ngtemplate-loader": "^2.0.1",
    "nightwatch": "^1.0.18",
